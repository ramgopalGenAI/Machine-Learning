n","scaler = StandardScaler()\n","X_train_scaled = scaler.fit_transform(X_train)\n","X_test_scaled = scaler.transform(X_test)\n","\n","# Define base estimators for Stacking\n","estimators_q44 = [\n","    ('rf', RandomForestClassifier(n_estimators=50, random_state=42, n_jobs=-1)),\n","    ('lr', LogisticRegression(solver='liblinear', random_state=42))\n","]\n","\n","# Define the meta-estimator\n","meta_estimator_q44 = LogisticRegression(solver='liblinear', random_state=42)\n","\n","# Initialize Stacking Classifier\n","stacking_clf_q44 = StackingClassifier(\n","    estimators=estimators_q44,\n","    final_estimator=meta_estimator_q44,\n","    cv=5,\n","    n_jobs=-1,\n","    verbose=1\n",")\n","\n","# Train the Stacking Classifier\n","print(\"Training Stacking Classifier with Random Forest and Logistic Regression...\")\n","stacking_clf_q44.fit(X_train_scaled, y_train)\n","print(\"Training complete.\")\n","\n","# Make predictions and calculate accuracy\n","y_pred_stacking_q44 = stacking_clf_q44.predict(X_test_scaled)\n","accuracy_stacking_q44 = accuracy_score(y_test, y_pred_stacking_q44)\n","print(f\"Stacking Classifier (RF + LR base) Accuracy: {accuracy_stacking_q44:.4f}\")\n","\n","# For comparison, let's also get accuracy of individual models if trained separately\n","# rf_clf_single_q44 = RandomForestClassifier(n_estimators=100, random_state=42, n_jobs=-1).fit(X_train_scaled, y_train)\n","# lr_clf_single_q44 = LogisticRegression(solver='liblinear', random_state=42).fit(X_train_scaled, y_train)\n","\n","# print(f\"Single RF Accuracy: {accuracy_score(y_test, rf_clf_single_q44.predict(X_test_scaled)):.4f}\")\n","# print(f\"Single LR Accuracy: {accuracy_score(y_test, lr_clf_single_q44.predict(X_test_scaled)):.4f}\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"iguFavyPPZkx","executionInfo":{"status":"ok","timestamp":1750320727955,"user_tz":-330,"elapsed":828,"user":{"displayName":"Ram Gopal Gupta","userId":"17103427756007212633"}},"outputId":"f09b0529-1e71-477b-b963-b18165eaab33"},"execution_count":18,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","--- Question 44: Stacking Classifier with Random Forest and Logistic Regression ---\n","Generated classification dataset: X.shape=(600, 10), y.shape=(600,)\n","Training Stacking Classifier with Random Forest and Logistic Regression...\n","Training complete.\n","Stacking Classifier (RF + LR base) Accuracy: 0.9056\n"]}]},{"cell_type":"markdown","source":["45. Train a Bagging Regressor with different levels of bootstrap samples and compare performance."],"metadata":{"id":"4wsj93PIPmBp"}},{"cell_type":"code","execution_count":17,"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":746},"id":"yo-IyYFwFk2U","executionInfo":{"status":"ok","timestamp":1750320712702,"user_tz":-330,"elapsed":4157,"user":{"displayName":"Ram Gopal Gupta","userId":"17103427756007212633"}},"outputId":"83403d48-3f1c-4a1f-d20b-1fd3958e8926"},"outputs":[{"output_type":"stream","name":"stdout","text":["\n","--- Question 45: Bagging Regressor - Effect of Bootstrap Samples (max_samples) ---\n","Generated regression dataset: X.shape=(300, 5), y.shape=(300,)\n","Training Bagging Regressor with varying `max_samples` (bootstrap sample size):\n","  Max_samples = 0.5: MSE = 36.0993\n","  Max_samples = 0.7: MSE = 35.2882\n","  Max_samples = 0.9: MSE = 36.5925\n","  Max_samples = 1.0: MSE = 36.3455\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 1000x600 with 1 Axes>"],"image/png":"\n"},"metadata":{}}],"source":["# %% [markdown]\n","# #### **45. Train a Bagging Regressor with different levels of bootstrap samples and compare performance.**\n","\n","# %%\n","print(\"\\n--- Question 45: Bagging Regressor - Effect of Bootstrap Samples (max_samples) ---\")\n","X_reg, y_reg = generate_sample_data_regression()\n","X_train_reg, X_test_reg, y_train_reg, y_test_reg = train_test_split(X_reg, y_reg, test_size=0.3, random_state=42)\n","\n","# max_samples: The number of samples to draw from X to train each base estimator\n","# Can be int (absolute number) or float (fraction of X)\n","max_samples_list = [0.5, 0.7, 0.9, 1.0] # Proportions of the training data\n","mses_max_samples = []\n","\n","base_tree_reg_q45 = DecisionTreeRegressor(random_state=42)\n","\n","print(\"Training Bagging Regressor with varying `max_samples` (bootstrap sample size):\")\n","for max_s in max_samples_list:\n","    bagging_reg_q45 = BaggingRegressor(\n","        estimator=base_tree_reg_q45,\n","        n_estimators=100,\n","        max_samples=max_s, # This parameter controls the bootstrap sample size\n","        random_state=42,\n","        n_jobs=-1\n","    )\n","    bagging_reg_q45.fit(X_train_reg, y_train_reg)\n","    y_pred_reg_q45 = bagging_reg_q45.predict(X_test_reg)\n","    mse = mean_squared_error(y_test_reg, y_pred_reg_q45)\n","    mses_max_samples.append(mse)\n","    print(f\"  Max_samples = {max_s:.1f}: MSE = {mse:.4f}\")\n","\n","# Plotting the results\n","plt.figure(figsize=(10, 6))\n","plt.plot([f'{s*100:.0f}%' for s in max_samples_list], mses_max_samples, marker='o', linestyle='-')\n","plt.title('Bagging Regressor MSE vs. Bootstrap Sample Size (max_samples)')\n","plt.xlabel('Proportion of Samples per Bootstrap')\n","plt.ylabel('Mean Squared Error (MSE)')\n","plt.grid(True)\n","plt.tight_layout()\n","plt.show()\n","\n"]}]}